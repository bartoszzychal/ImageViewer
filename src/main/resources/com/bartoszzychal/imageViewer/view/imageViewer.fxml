<?xml version="1.0" encoding="UTF-8"?>

<?scenebuilder-preview-i18n-resource ../bundle/base.properties?>
<?scenebuilder-stylesheet ../css/standard.css?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>

<AnchorPane fx:id="anchorPane" prefHeight="800.0" prefWidth="800" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bartoszzychal.imageViewer.controller.ImageViewerController">
   <children>
      <ScrollPane hbarPolicy="AS_NEEDED" layoutY="50.0" vbarPolicy="ALWAYS">
         <content>
            <TilePane fx:id="tile" alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT" prefHeight="160.0" prefWidth="800.0" tileAlignment="TOP_LEFT" />
         </content>
      </ScrollPane>
      <ScrollPane fx:id="scrollPane" hbarPolicy="AS_NEEDED" layoutY="257.0" prefHeight="545.0" prefWidth="819.0" vbarPolicy="AS_NEEDED">
      	<ImageView fx:id="imageViewViewer" cache="true" disable="true" fitHeight="585.0" fitWidth="818.0" pickOnBounds="true" preserveRatio="true" smooth="true" />
      </ScrollPane>
      <Button fx:id="directoryButton" layoutX="8.0" layoutY="8.0" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#setDirectory" prefHeight="34.0" prefWidth="150.0" text="Select directory" />
      <Label fx:id="directoryLabel" layoutX="165.0" layoutY="13.0" prefHeight="24.0" prefWidth="640.0" />
      <Button fx:id="previousButton" alignment="CENTER" layoutX="14.0" layoutY="219.0" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#previousPicture" prefHeight="34.0" prefWidth="150.0" text="Previous" />
      <Button fx:id="nextButton" alignment="CENTER" layoutX="655.0" layoutY="219.0" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#nextPicture" prefHeight="34.0" prefWidth="150.0" text="Next" />
      <Label fx:id="fileName" layoutX="335.0" layoutY="224.0" prefHeight="24.0" prefWidth="300.0" />
      <Button fx:id="slideButton" alignment="BASELINE_LEFT" contentDisplay="TOP" layoutX="172.0" layoutY="219.0" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#slideshow" prefHeight="34.0" prefWidth="150.0" text="Start" />
      <TextField fx:id="timeTextField" alignment="CENTER" layoutX="271.0" layoutY="219.0" prefHeight="34.0" prefWidth="51.0" />
   </children>
</AnchorPane>
